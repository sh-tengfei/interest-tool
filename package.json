    "dayjs": "^1.11.11",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.1",
    "koa-session": "^6.4.0",
    "mongoose": "^8.4.3",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.3",
    "pm2": "^5.4.1",
    "request": "^2.88.2",
    "silly-datetime": "^0.1.2",
    "svg-captcha": "^1.4.0"
