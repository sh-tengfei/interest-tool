{
    "restartable": false,
    "ignore": [
      ".git",
      "dist",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "colours": true,
    "exec": "npm run lint & babel-node src/main.js",
    "execMap": {
      "js": "node --harmony"
    },
    "events": {

    },
    "watch": [
      "src"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json",
    "delay": 100
  }
